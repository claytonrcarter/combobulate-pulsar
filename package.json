{
  "name": "combobulate",
  "main": "./lib/combobulate",
  "version": "0.0.0",
  "description": "Structural navigation and editting in Pulsar, powered by tree-sitter",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "combobulate:add-cursor-to-all-siblings",
      "combobulate:add-cursor-to-next-sibling",
      "combobulate:add-cursor-to-previous-sibling",
      "combobulate:move-to-current-function",
      "combobulate:move-to-current-function-end",
      "combobulate:move-to-larger-syntax-node",
      "combobulate:move-to-smaller-syntax-node",
      "combobulate:move-to-next-sibling",
      "combobulate:move-to-previous-sibling",
      "combobulate:select-current-function",
      "combobulate:select-current-node",
      "combobulate:select-next-sibling",
      "combobulate:select-previous-sibling",
      "combobulate:swap-with-next-sibling",
      "combobulate:swap-with-previous-sibling",
      "combobulate:transpose-nodes"
    ]
  },
  "repository": "https://github.com/claytonrcarter/combobulate-pulsar",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "dedent": "^1.5.1",
    "prettier": "^3.2.4"
  }
}
